    <sql id="selectFdEquipmentDictVo">
        select e.id, e.code e_code, e.name e_name, e.supplier_id , e.speci, e.model,
            e.price, e.create_by, e.create_time, e.update_by, e.update_time,e.del_flag,
            e.referred_name, e.use_name, e.factory_id, e.storeroom_id, e.finance_category_id, e.unit_id,
            e.register_name, e.register_no, e.medical_name, e.medical_no, e.period_date, e.successful_type,
            e.successful_no, e.successful_price, e.sale_price, e.package_speci, e.producer, e.equipment_level,
            e.register_level, e.risk_level, e.firstaid_level, e.doctor_level, e.brand, e.useto, e.quality,
            e.function, e.is_way, e.udi_no, e.permit_no, e.country_no, e.country_name, e.description,
            e.is_use, e.is_procure, e.is_monitor, e.is_gz, e.is_follow, e.location_id,
            e.image_url, e.sunshine_code, e.is_billing,
            s.id sid, s.code supplier_code,s.name supplier_name,
            w.warehouse_category_id wid, w.warehouse_category_code w_code,w.warehouse_category_name w_name,
            f.factory_id fid,f.factory_code f_code,f.factory_name f_name,
            u.unit_id uid,u.unit_code u_code,u.unit_name u_name,cwfl.finance_category_code cwfl_code,cwfl.finance_category_name cwfl_name,
            l.location_id lid, l.location_name l_name
        from fd_equipment_dict e
        left join fd_supplier s on e.supplier_id = s.id
        left join fd_warehouse_category w on e.storeroom_id = w.warehouse_category_id
        left join fd_factory f on e.factory_id = f.factory_id
        left join fd_unit u on u.unit_id = e.unit_id
        left join fd_finance_category cwfl on e.finance_category_id = cwfl.finance_category_id
        left join fd_location l on e.location_id = l.location_id
    </sql>

    <select id="selectFdEquipmentDictList" parameterType="FdEquipmentDict" resultMap="FdEquipmentDictResult">
        <include refid="selectFdEquipmentDictVo"/>
        <where>
            e.del_flag != 1
            <if test="code != null  and code != ''"> and e.code = #{code}</if>
            <if test="name != null  and name != ''"> and e.name like concat('%', #{name}, '%')</if>
            <if test="supplierId != null "> and e.supplier_id = #{supplierId}</if>
            <if test="id != null "> and e.id = #{id}</if>
            <if test="udiNo != null "> and e.udi_no = #{udiNo}</if>
            <if test="isGz != null "> and e.is_gz = #{isGz}</if>
            <if test="isFollow != null and isFollow != ''"> and e.is_follow = #{isFollow}</if>
            <if test="beginDate != null "> and e.create_time &gt;= #{beginDate}</if>
            <if test="endDate != null "> and e.create_time &lt;= #{endDate}</if>
        </where>
    </select>

    <select id="selectFdEquipmentDictById" parameterType="Long" resultMap="FdEquipmentDictResult">
        <include refid="selectFdEquipmentDictVo"/>
        where e.id = #{id} and (e.del_flag = 0 or e.del_flag is null)
    </select>
    
    <select id="selectFdEquipmentDictByCode" parameterType="String" resultMap="FdEquipmentDictResult">
