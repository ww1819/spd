    <sql id="selectEquipmentStorageDetailVo">
        select detail_id, storage_id, equipment_code, equipment_name, category_code, specification, model, 
               spec, model_no, quantity, total_boxes, unit_price, total_price, amount, amount_in_words,
               inspection_no, registration_no, serial_no, production_date, usage_period,
               manufacturer, manufacturer_contact, dealer, dealer_contact,
               packaging_type, packaging_status, packaging_damage, appearance, accessories,
               tech_param_match, manual_count, certificate_count, packing_list_count, acceptance_form_count,
               acceptance_result, acceptance_date, remark 
        from equipment_storage_detail
    </sql>

    <select id="selectEquipmentStorageDetailList" parameterType="Long" resultMap="EquipmentStorageDetailResult">
        <include refid="selectEquipmentStorageDetailVo"/>
        where storage_id = #{storageId}
        order by detail_id
    </select>
        
    <insert id="insertEquipmentStorageDetail" parameterType="EquipmentStorageDetail" useGeneratedKeys="true" keyProperty="detailId">
        insert into equipment_storage_detail
